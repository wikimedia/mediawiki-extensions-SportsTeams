{
	"root": true,
	"extends": [
		"wikimedia/client",
		"wikimedia/mediawiki"
	],
	"rules": {
		"camelcase": "off",
		"eqeqeq": "off",
		"no-alert": "off",
		"no-implicit-globals": "off",
		"no-undef": "off",
		"no-unused-vars": "off",
		"mediawiki/no-cookie": "warn",
		"no-var": "warn",
		"prefer-const": "warn"
	}
}
